<div class="row">
  <div class="col-sm-offset-3 col-sm-6 col-md-offset-4 col-md-4 central">
  <h1>Log in</h1>
  <%= bootstrap_form_for(:session, url: login_path) do |f| %>

    <%= f.email_field :email %>

    <%= f.password_field :password %>

    <%= f.submit "Log in" %>
  <% end %>
  </div>
</div>
